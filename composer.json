{
	"name": "fleshgrinder/comparable",
	"description": "Type-safe comparison for PHP.",
	"license": "Unlicense",
	"authors": [
		{
			"name": "Richard Fussenegger",
			"email": "fleshgrinder@users.noreply.github.com"
		}
	],
	"require": {
		"php": "^7.0",
		"fleshgrinder/equalable": "^1.0",
		"fleshgrinder/format": "^1.1",
		"fleshgrinder/value": "^1.0",
		"fleshgrinder/core": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.0"
	},
	"autoload": {
		"psr-4": {
			"Fleshgrinder\\Core\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Fleshgrinder\\Core\\": [
				"src/",
				"tests/"
			]
		}
	},
	"scripts": {
		"post-install-cmd": "git update-index --skip-worktree .idea/php-comparable.iml",
		"post-update-cmd": "git update-index --skip-worktree .idea/php-comparable.iml",
		"test": "phpunit --colors=always --testdox"
	}
}
